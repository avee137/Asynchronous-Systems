	def callFailureFunctionBasedOnTrigger_PHASE_3(triggerName,trigger_n,chkptShuttle):
		logger.info("callFailureFunctionBasedOnTrigger triggerName: "+str(triggerName)+", trigger_n: "+str(trigger_n))
		
		if(triggerName == "crash"):
			crash()
		elif(triggerName == "truncate_history"):
			truncate_history(trigger_n)
		elif(triggerName == "sleep"):
			sleep(trigger_n)
		elif(triggerName == "increment_slot"):
			increment_slot()
		elif(triggerName == "extra_op"):
			return extra_op()
		elif(triggerName == "drop"):
			self.drop_failure_flag=True
		# if(triggerName == "invalid_order_sig"):
		# 	return drop_result_stmt(shuttle)
		# if(triggerName == "invalid_result_sig"):
		# 	return drop_result_stmt(shuttle)
		if(triggerName == "drop_checkpt_stmts"):
			return drop_checkpt_stmts(chkptShuttle)
		

	def checkForPossibleFailure(client,validTriggerSet):
		logger.info("CheckForPossibleFailure : "+str(self.counters)+", self.failureDS : "+str(self.failureDS)+", client : "+str(client))
		if(self.failureDS == None or len(self.failureDS)==0) :
			print("CheckForPossibleFailure returning 1")
			return None
		replicaIndex = self.replicas.index(self.currentReplica)
		print("1. ended up here")
		currentConfiguration = self.configuration
		if(currentConfiguration not in self.failureDS):
			print("CheckForPossibleFailure returning 1.1")
			return None
		failureDsReplicaDict = self.failureDS[currentConfiguration]["replica"]
		print("2. ended up here")
		logger.debug("the dictionary: ",failureDsReplicaDict)
		if(replicaIndex not in failureDsReplicaDict):
			print("CheckForPossibleFailure returning 2")
			return None
		print("3 ended up here")
		failureLists = self.failureDS[currentConfiguration]["replica"][replicaIndex]
		print("4. ended up here")
		for obj in failureLists:
			triggerName = obj["triggerName"]
			clientObj = obj["client"]
			if(clientObj == None):
				print("CheckForPossibleFailure returning 3")
				return None
			messageNumber=obj["messageNumber"]
			triggerFailure=obj["triggerFailure"]
			if(clientObj != self.clients.index(client)):
				logger.debug("client index is different")
				continue
			if(triggerName not in validTriggerSet):
				logger.debug("trigger name is not in the set:" +str(validTriggerSet))
				continue
			logger.debug("reached here!")
			if(self.counters[client]["directRequestCounter"] == messageNumber and triggerName=="client_request"):
				return triggerFailure
			elif(self.counters[client]["forwardRequestCounter"] == messageNumber and triggerName=="forwarded_request"):
				return triggerFailure
			elif(self.counters[client]["shuttleCounter"] == messageNumber and triggerName=="shuttle"):
				return triggerFailure
			elif(self.counters[client]["resultShuttleCounter"] == messageNumber and triggerName=="result_shuttle"):
				return triggerFailure
			logger.debug("before returning")
		print("CheckForPossibleFailure returning 4")
		return None

	def checkForPossibleFailure_PHASE_3(validTriggerSet):
		'''
		self.counters["wedge_requestCounter"]=0
		self.counters["new_configurationCounter"]=0
		self.counters["checkpointCounter"]=0
		self.counters["completed_checkpointCounter"]=0
		self.counters["get_running_stateCounter"]=0
		self.counters["catch_upCounter"]

		'''
		logger.info("checkForPossibleFailure_PHASE_3 called with configuration number: "+str(self.configuration)+",validTriggerSet : "+str(validTriggerSet)+", failureDS: "+str(self.failureDS)+", counters: "+str(self.counters))
		if(self.failureDS == None or len(self.failureDS)==0) :
			logger.info("returning 1")
			return None,None
		replicaIndex = self.replicas.index(self.currentReplica)
		if(self.configuration not in self.failureDS):
			logger.info("returning 2")
			return None,None
		failureDsReplicaDict = self.failureDS[self.configuration]["replica"]
		logger.info("failureDsReplicaDict the dictionary: ",failureDsReplicaDict)

		if((failureDsReplicaDict is None) or (replicaIndex not in failureDsReplicaDict)):
			logger.info("returning 3, =>"+str(self.failureDS[self.configuration])+", =>"+str(self.failureDS[self.configuration]["replica"]))
			return None,None
		failureLists = self.failureDS[self.configuration]["replica"][replicaIndex]
		for obj in failureLists:
			triggerName = obj["triggerName"]
			messageNumber=obj["messageNumber"]
			triggerFailure=obj["triggerFailure"]
			triggerFailure_m=None
			if("triggerFailure_m" in obj):
				triggerFailure_m=obj["triggerFailure_m"]
			if(triggerName not in validTriggerSet):
				logger.debug("trigger name is not in the set:" +str(validTriggerSet))
				continue
			logger.debug("reached here!")
			if(self.counters["wedge_requestCounter"] == messageNumber and triggerName=="wedge_request"):
				return triggerFailure,triggerFailure_m
			elif(triggerName=="new_configuration"): #because execution will only reach here if there is some failure for this configuration and one of it is new_configuration
				return triggerFailure,triggerFailure_m
			elif(self.counters["checkpointCounter"] == messageNumber and triggerName=="checkpoint"):
				return triggerFailure,triggerFailure_m
			elif(self.counters["completed_checkpointCounter"] == messageNumber and triggerName=="completed_checkpoint"):
				return triggerFailure,triggerFailure_m
			elif(self.counters["get_running_stateCounter"] == messageNumber and triggerName=="get_running_state"):
				return triggerFailure,triggerFailure_m
			elif(self.counters["catch_upCounter"] == messageNumber and triggerName=="catch_up"):
				return triggerFailure,triggerFailure_m
			logger.debug("before returning")
		logger.info("returning 4")
		return None,None

