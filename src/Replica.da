		else:
			logger.info("sending to olympus. running state : "+str(self.dictionary_data))
			send(('RUNNING_STATE',self.dictionary_data,), to=self.olympus)
			
		logger.info("************FINAL_RUNNING_STATE sent to Olympus********")
		
